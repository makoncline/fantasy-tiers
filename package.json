{
  "name": "fantasy-tiers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fetch-data": "npx tsx src/lib/fetchTeamData.ts && npx tsx src/lib/fetchPlayerData.ts && npx tsx src/lib/fetchRankingData.ts",
    "fetch-ranking-data": "npx tsx src/lib/fetchRankingData.ts",
    "parse-data": "npx tsx src/lib/parseTeamData.ts && npx tsx src/lib/parsePlayerData.ts && npx tsx src/lib/parseRankingData.ts && npx tsx src/lib/aggregatePlayerData.ts",
    "aggregate:fp": "node --import=tsx scripts/fp/aggregate-fantasypros.ts",
    "fetch:fp": "node --import=tsx scripts/fp/fetch-fantasypros-all.ts",
    "fetch:sleeper:projections": "npx tsx scripts/sleeper/fetchProjections.ts",
    "fetch:sleeper:players-meta": "npx tsx scripts/sleeper/fetchPlayersMeta.ts",
    "build:combined-aggregate": "npx tsx scripts/sleeper/buildCombinedAggregate.ts",
    "pipeline:fetch-all": "npx tsx scripts/pipelines/fetchAll.ts",
    "pipeline:build-aggregates": "npx tsx scripts/pipelines/buildAggregates.ts",
    "fetch:sleeper": "pnpm run fetch:sleeper:projections && pnpm run fetch:sleeper:players-meta",
    "fetch:fp": "node --import=tsx scripts/fp/fetch-fantasypros-all.ts",
    "fetch:borischen": "npx tsx src/lib/fetchRankingData.ts",
    "agg:sleeper": "pnpm run build:combined-aggregate",
    "agg:fp": "pnpm run aggregate:fp",
    "agg:borischen": "npx tsx src/lib/parseRankingData.ts",
    "fetch:all": "pnpm run fetch:sleeper && pnpm run fetch:fp && pnpm run fetch:borischen",
    "agg:all": "pnpm run agg:fp && pnpm run agg:sleeper",
    "temp:check-name-coverage": "npx tsx scripts/temp/checkNameCoverage.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.85.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "fuse.js": "^7.1.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "react": "^19",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.16",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jsdom": "^26.1.0",
    "postcss": "^8",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=22.18.0"
  }
}
